{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "react"],
  "rules": {
    "jsx-a11y/no-autofocus": 0,
    "react/prop-types": 0,
    "react/display-name": 0,
    "no-empty-pattern": 0,
    "no-unused-vars": "warn" 
  }
}
